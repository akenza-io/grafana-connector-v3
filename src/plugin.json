{
    "type": "datasource",
    "name": "Akenza Connector",
    "id": "akenza-datasource",
    "metrics": true,
    "info": {
        "description": "Enables Akenza as Data Source",
        "author": {
            "name": "akenza",
            "url": "https://akenza.io"
        },
        "keywords": ["akenza.io", "akenza", "IoT", "Internet of Things"],
        "logos": {
            "small": "img/akenza-badge-light.png",
            "large": "img/akenza-io-logo.svg"
        },
        "links": [
            {
                "name": "Website",
                "url": "https://github.com/akenza-io/grafana-connector-v3"
            },
            {
                "name": "License",
                "url": "https://github.com/akenza-io/grafana-connector-v3/blob/master/LICENSE"
            }
        ],
        "screenshots": [],
        "version": "%VERSION%",
        "updated": "%TODAY%"
    },
    "dependencies": {
        "grafanaVersion": "7.x.x",
        "plugins": []
    },
    "routes": [
        {
            "path": "akenza-secure",
            "url": "{{ .JsonData.baseUrl }}",
            "headers": [
                {
                    "name": "x-api-key",
                    "content": "{{ .SecureJsonData.apiKey }}"
                }
            ]
        },
        {
            "path": "akenza-insecure",
            "url": "{{ .JsonData.baseUrl }}",
            "headers": [
                {
                    "name": "x-api-key",
                    "content": "{{ .JsonData.apiKey }}"
                }
            ]
        }
    ]
}
